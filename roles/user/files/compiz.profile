
[commands]
s0_command0 = exo-open --launch FileManager
s0_command1 = charm
s0_command2 = pidgin
s0_command3 = google-chrome
s0_command4 = gvim
s0_command5 = xfce4-popup-whiskermenu
s0_command6 = xfce4-terminal -e 'tmux -2'
s0_command7 = xflock4
s0_command8 = gvim ~/Decipher/Journal/$(date +"%Y%m%d").md
s0_run_command0_key = <Super>f
s0_run_command1_key = <Super>c
s0_run_command2_key = <Super>p
s0_run_command3_key = <Super>b
s0_run_command4_key = <Super>v
s0_run_command5_key = <Super>x
s0_run_command6_key = <Super>t
s0_run_command7_key = <Shift><Control><Alt>l
s0_run_command8_key = <Super>j

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;decor;put;wall;staticswitcher;screenshot;shift;move;place;vpswitch;gnomecompat;grid;resize;imgpng;commands;wobbly;scale;expo;
s0_outputs = 1600x900+0+0;1600x900+1600+0;
s0_focus_prevention_level = 0
s0_unmaximize_window_key = <Control><Super>Down
s0_unmaximize_or_minimize_window_key = Disabled
s0_window_menu_key = Disabled
s0_toggle_window_maximized_key = Disabled
s0_toggle_window_shaded_key = Disabled
s0_hsize = 3
s0_vsize = 3

[firepaint]
s0_num_particles = 100

[gnomecompat]
s0_command_screenshot = mate-screenshot
s0_command_window_screenshot = mate-screenshot --window
s0_command_terminal = mate-terminal
s0_run_command_terminal_key = Disabled

[grid]
s0_put_left_key = <Control><Super>Left
s0_put_right_key = <Control><Super>Right
s0_put_maximize_key = <Alt><Super>Up
s0_put_restore_key = Disabled
s0_top_left_corner_action = 0
s0_top_edge_action = 5
s0_top_right_corner_action = 0
s0_left_edge_action = 0
s0_right_edge_action = 0
s0_bottom_left_corner_action = 0
s0_bottom_right_corner_action = 0
s0_draw_indicator = false
s0_draw_stretched_window = false

[move]
s0_initiate_key = Disabled
s0_snapoff_distance = 20

[put]
s0_put_next_output_key = <Super>r
s0_speed = 20.000000

[resize]
s0_initiate_key = Disabled

[ring]
s0_next_key = Disabled
s0_next_all_key = <Super>Tab

[scale]
s0_spacing = 0

[shift]
s0_initiate_key = Disabled
s0_initiate_all_key = <Super>Tab
s0_next_key = Disabled
s0_prev_key = Disabled
s0_next_all_key = Disabled
s0_prev_all_key = Disabled

[staticswitcher]
s0_next_key = Disabled
s0_prev_key = Disabled
s0_next_all_key = <Alt>Tab
s0_prev_all_key = <Shift><Alt>Tab
s0_saturation = 0

[vpswitch]
s0_switch_to_1_key = <Super>1
s0_switch_to_5_key = <Super>2
s0_switch_to_9_key = <Super>3

[winrules]
s0_size_matches = ;
s0_size_width_values = 0;
s0_size_height_values = 0;

[wobbly]
s0_grid_resolution = 1
